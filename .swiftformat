--guardelse next-line
--header strip
--hexgrouping 4,8
--hexliteralcase lowercase
--ifdef no-indent
--importgrouping alpha
--indent 4
--indentcase true
--lifecycle
--linebreaks lf
--markextensions always
--marktypes always
--maxwidth 120
--modifierorder
--nevertrailing
--nospaceoperators
